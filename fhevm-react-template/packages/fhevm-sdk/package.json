{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps - framework agnostic",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --format=cjs",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --outfile=dist/index.esm.js --format=esm",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "blockchain",
    "privacy",
    "web3",
    "sdk",
    "framework-agnostic"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "dependencies": {
    "fhevmjs": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zama-ai/fhevm-react-template.git",
    "directory": "packages/fhevm-sdk"
  }
}
